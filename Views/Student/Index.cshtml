@model List<student>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>Index</title>
</head>
<body>
    <h1>Student List</h1>
    <table>
        <thead>
            <tr><td>Id</td><td>Name</td></tr>
        </thead>
        <tbody>
            @foreach(var i in Model) {
            <tr><td>@i.Id</td><td>@i.Name</td></tr>
            }
        </tbody>
    </table>
</body>
</html>